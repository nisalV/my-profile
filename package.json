{
  "name": "my-profile",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@stylexjs/stylex": "^0.5.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "webpack serve --env env=dev --config webpack.config.js",
    "start:prod": "webpack serve --env env=prod --config webpack.config.js",
    "start-secure": "webpack-dev-server --https --config webpack.config.js --host dev.local --env env=dev mode=secure",
    "build": "webpack --env env=dev --config webpack.config.js",
    "build:prod": "webpack --env env=prod --config webpack.config.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@stylexjs/eslint-plugin": "^0.5.1",
    "@stylexjs/webpack-plugin": "^0.5.1",
    "@types/node": "^20.11.22",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "copy-webpack-plugin": "^12.0.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  }
}
